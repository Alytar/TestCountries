<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             tools:ignore="UnusedAttribute">

    <!-- For sizes description
        100    Extra Light or Ultra Light
        200    Light or Thin
        300    Book or Demi
        400    Normal or Regular
        500    Medium
        600    Semibold, Demibold
        700    Bold
        800    Black, Extra Bold or Heavy
        900    Extra Black, Fat, Poster or Ultra Black
    -->

    <font
            android:font="@font/montserrat_alternates_extra_light"
            android:fontStyle="normal"
            android:fontWeight="100"
            app:font="@font/montserrat_alternates_extra_light"
            app:fontStyle="normal"
            app:fontWeight="100"/>

    <font
            android:font="@font/montserrat_alternates_extra_light_italic"
            android:fontStyle="italic"
            android:fontWeight="100"
            app:font="@font/montserrat_alternates_extra_light_italic"
            app:fontStyle="italic"
            app:fontWeight="100"/>

    <font
            android:font="@font/montserrat_alternates_light"
            android:fontStyle="normal"
            android:fontWeight="200"
            app:font="@font/montserrat_alternates_light"
            app:fontStyle="normal"
            app:fontWeight="200"/>

    <font
            android:font="@font/montserrat_alternates_light_italic"
            android:fontStyle="italic"
            android:fontWeight="200"
            app:font="@font/montserrat_alternates_light_italic"
            app:fontStyle="italic"
            app:fontWeight="200"/>

    <font
            android:font="@font/montserrat_alternates_thin"
            android:fontStyle="normal"
            android:fontWeight="200"
            app:font="@font/montserrat_alternates_thin"
            app:fontStyle="normal"
            app:fontWeight="200"/>

    <font
            android:font="@font/montserrat_alternates_thin_italic"
            android:fontStyle="italic"
            android:fontWeight="200"
            app:font="@font/montserrat_alternates_thin_italic"
            app:fontStyle="italic"
            app:fontWeight="200"/>

    <font
            android:font="@font/montserrat_alternates_regular"
            android:fontStyle="normal"
            android:fontWeight="400"
            app:font="@font/montserrat_alternates_regular"
            app:fontStyle="normal"
            app:fontWeight="400"/>

    <font
            android:font="@font/montserrat_alternates_regular_italic"
            android:fontStyle="italic"
            android:fontWeight="400"
            app:font="@font/montserrat_alternates_regular_italic"
            app:fontStyle="italic"
            app:fontWeight="400"/>

    <font
            android:font="@font/montserrat_alternates_medium"
            android:fontStyle="normal"
            android:fontWeight="500"
            app:font="@font/montserrat_alternates_medium"
            app:fontStyle="normal"
            app:fontWeight="500"/>

    <font
            android:font="@font/montserrat_alternates_medium_italic"
            android:fontStyle="italic"
            android:fontWeight="500"
            app:font="@font/montserrat_alternates_medium_italic"
            app:fontStyle="italic"
            app:fontWeight="500"/>

    <font
            android:font="@font/montserrat_alternates_semibold"
            android:fontStyle="normal"
            android:fontWeight="600"
            app:font="@font/montserrat_alternates_semibold"
            app:fontStyle="normal"
            app:fontWeight="600"/>

    <font
            android:font="@font/montserrat_alternates_semibold_italic"
            android:fontStyle="italic"
            android:fontWeight="600"
            app:font="@font/montserrat_alternates_semibold_italic"
            app:fontStyle="italic"
            app:fontWeight="600"/>

    <font
            android:font="@font/montserrat_alternates_bold"
            android:fontStyle="normal"
            android:fontWeight="700"
            app:font="@font/montserrat_alternates_bold"
            app:fontStyle="normal"
            app:fontWeight="700"/>

    <font
            android:font="@font/montserrat_alternates_bold_italic"
            android:fontStyle="italic"
            android:fontWeight="700"
            app:font="@font/montserrat_alternates_bold_italic"
            app:fontStyle="italic"
            app:fontWeight="700"/>

    <font
            android:font="@font/montserrat_alternates_black"
            android:fontStyle="normal"
            android:fontWeight="800"
            app:font="@font/montserrat_alternates_black"
            app:fontStyle="normal"
            app:fontWeight="800"/>

    <font
            android:font="@font/montserrat_alternates_black_italic"
            android:fontStyle="italic"
            android:fontWeight="800"
            app:font="@font/montserrat_alternates_black_italic"
            app:fontStyle="italic"
            app:fontWeight="800"/>

    <font
            android:font="@font/montserrat_alternates_extra_bold"
            android:fontStyle="normal"
            android:fontWeight="900"
            app:font="@font/montserrat_alternates_extra_bold"
            app:fontStyle="normal"
            app:fontWeight="900"/>

    <font
            android:font="@font/montserrat_alternates_extra_bold_italic"
            android:fontStyle="italic"
            android:fontWeight="900"
            app:font="@font/montserrat_alternates_extra_bold_italic"
            app:fontStyle="italic"
            app:fontWeight="900"/>

</font-family>